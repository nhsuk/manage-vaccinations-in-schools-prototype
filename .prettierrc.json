{
  "semi": false,
  "singleQuote": true,
  "quoteProps": "preserve",
  "tabWidth": 2,
  "trailingComma": "none",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "embeddedLanguageFormatting": "off"
      }
    },
    {
      "files": "*.scss",
      "options": {
        "printWidth": 120,
        "singleQuote": false
      }
    }
  ]
}
