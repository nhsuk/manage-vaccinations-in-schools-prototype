{% extends "_layouts/form.njk" %}

{% set title = __("session.open.title") %}

{% block form %}
  {{ dateInput({
    fieldset: {
      legend: {
        classes: "nhsuk-fieldset__legend--l",
        html: heading({
          caption: session.location.name,
          title: title
        })
      }
    },
    hint: { text: __("session.open.hint") },
    decorate: "session.open_"
  }) }}
{% endblock %}
