{% extends "_layouts/form.njk" %}

{% set title = __("session.edit.urn.title") %}

{% block form %}
  {{ xGovukAutocomplete({
    id: "urn",
    name: "[session][urn]",
    value: session.urn,
    allowEmpty: false,
    label: {
      classes: "nhsuk-label--l",
      isPageHeading: true,
      text: title
    },
    items: urnItems
  }) | replace("govuk-", "nhsuk-") }}
{% endblock %}
