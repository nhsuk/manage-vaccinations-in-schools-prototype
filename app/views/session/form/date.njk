{% extends "_layouts/form.njk" %}

{% set title = __("session.edit.date.title") %}

{% block form %}
  {{ dateInput({
    fieldset: {
      legend: {
        classes: "nhsuk-fieldset__legend--m",
        html: heading({
          title: title
        })
      }
    },
    hint: { text: __("session.date.hint") },
    decorate: "session.date_"
  }) }}
{% endblock %}
