{% extends "_layouts/form.njk" %}

{% set title = __("session.school.title") %}

{% block form %}
  {{ appHeading({
    title: title
  }) }}

  {{ select({
    label: { text: __("session.school_id.title") },
    items: locationItems(schools, session.school_id),
    decorate: "session.school_id",
    attributes: {
      "data-module": "app-autocomplete"
    }
  }) }}
{% endblock %}
