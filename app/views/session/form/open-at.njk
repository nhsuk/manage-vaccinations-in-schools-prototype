{% extends "_layouts/form.njk" %}

{% set title = __("session.openAt.title") %}

{% block form %}
  {{ dateInput({
    fieldset: {
      legend: {
        classes: "nhsuk-fieldset__legend--l",
        html: appHeading({
          caption: session.location.name,
          title: title
        })
      }
    },
    hint: { text: __("session.openAt.hint") },
    decorate: "session.openAt_"
  }) }}
{% endblock %}
