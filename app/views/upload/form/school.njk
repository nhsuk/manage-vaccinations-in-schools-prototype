{% extends "_layouts/form.njk" %}

{% set title = __("upload.school.title") %}

{% block form %}
  {{ select({
    label: {
      html: appHeading({
        title: title
      })
    },
    items: schoolItems(data.schools, upload.school_id),
    decorate: "upload.school_id",
    attributes: {
      "data-module": "app-autocomplete"
    }
  }) }}
{% endblock %}
