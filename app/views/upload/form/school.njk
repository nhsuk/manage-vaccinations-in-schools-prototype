{% extends "_layouts/form.njk" %}

{% set title = __("upload.school.title") %}

{% block form %}
  {{ xGovukAutocomplete({
    id: "school-urn",
    name: "[upload][school_urn]",
    value: upload.school_urn,
    allowEmpty: false,
    label: {
      html: heading({
        title: title
      })
    },
    items: schoolItems(data.schools)
  }) | replace("govuk-", "nhsuk-") }}
{% endblock %}
