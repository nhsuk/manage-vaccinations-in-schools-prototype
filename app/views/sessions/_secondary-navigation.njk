{% set navigationItems = [] %}
{% for item, patients in patients | groupBy("consent") %}
  {% set navigationItems = navigationItems | push({
    text: (__("consent." + item + ".tab") + " " + count(patients.length)) | safe,
    href: "?consent=" + item,
    current: item == consent
  }) %}
{% endfor %}

{{ xGovukSecondaryNavigation({
  classes: "nhsuk-u-margin-bottom-4",
  items: navigationItems
}) }}
